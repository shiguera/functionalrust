% -------------------------------------------------------------------------------
% Nota: Cuadro para observaciones, con marco, título y sombra
% Utilización:
% \begin{Nota}[Título]
%    ... contenido
% \end{Nota}
% El título es opcional
% -------------------------------------------------------------------------------
\RequirePackage{adjustbox}
\definecolor{MedGray}{HTML}{f0f0f0}
\definecolor{FrameBlack}{HTML}{000000}
\definecolor{DarkGray}{HTML}{101010}

% ---------------------------------------------------------------
% Nota:
% Utilización:
%       Nota[Titulo]
\newtcolorbox{Nota}[1][Nota]{%
	center,
	enhanced,
	before skip=10mm,after skip=2mm, 
	width=\textwidth, boxrule=0.2mm, % width of the sticky note
	colback=MedGray, colframe=DarkGray, % Colors
	attach boxed title to top left={xshift=0cm,yshift*=0mm-\tcboxedtitleheight},
	varwidth boxed title*=-3cm,
	% The titlebox:
	boxed title style={frame code={%
			\path[left color=FrameBlack,right color=FrameBlack,
			middle color=FrameBlack]
			([xshift=-0mm]frame.north west) -- ([xshift=0mm]frame.north east)
			[rounded corners=0mm]-- ([xshift=0mm,yshift=0mm]frame.north east)
			-- (frame.south east) -- (frame.south west)
			-- ([xshift=0mm,yshift=0mm]frame.north west)
			[sharp corners]-- cycle;
		},interior engine=empty,
	},
	sharp corners,rounded corners=southeast,arc is angular,arc=3mm,
	% The "folded paper" in the bottom right corner:
	underlay={%
		\path[fill=MedGray!80!black] ([yshift=3mm]interior.south east)--++(-0.4,-0.1)--++(0.1,-0.2);
		\path[draw=FrameBlack,shorten <=-0.05mm,shorten >=-0.05mm,color=FrameBlack] ([yshift=3mm]interior.south east)--++(-0.4,-0.1)--++(0.1,-0.2);
	},
	drop fuzzy shadow, % Shadow
	fonttitle=\bfseries, 
	title={#1}
}

