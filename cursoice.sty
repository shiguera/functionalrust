% ----------------------------------------------------------------------
% EjemploCodigo: cuadro para ejemplos de código fuente
% Utilización:
% \begin{EjemploCodigo}[Título del cuadro]{label}
%    ... código del programa
% \end{EjemploCodigo}
% El título es opcional, pero la etiqueta no, así que hay que poner alguna etiqueta entre
% llaves o las llaves vacías, si no, se perderá el primer carácter del código. 
% En las etiquetas no se admite el carácter guion bajo
\newtcblisting[auto counter]{EjemploCodigo}[2][]{
	label={#2},
	bicolor, arc=15pt, colframe=black, colback=white, colbacklower=white,
	bottomrule=0.8pt, toprule=0.8pt, leftrule=0.8pt, rightrule=0.8pt,
	left=2pt, top=2pt, bottom=2pt, right=2pt, 
	fonttitle=\sffamily\bfseries,
	title={Ejemplo \thetcbcounter},
	sharp corners, 
	attach boxed title to top left,
	boxed title style={
		enhanced,
		colframe=black,
		colback=black,
		arc=5pt,
		bottomrule=0pt,
		rightrule=0pt,
		sharp corners,
	},
	interior style={},
	frame style={
		line width=0.8pt,
		left color=black,
		right color=black,
		fill=none},
	overlay unbroken and first={
		\node[anchor=west,font=\textbf\sffamily,text width=10cm]
		at (title.east) {{\strut\color{black} #1}};
	},
	listing only,
	listing options= {
		language=C, frame=none, basicstyle=\ttfamily,
		aboveskip=0pt,belowskip=0pt, lineskip=-0.1em, showstringspaces=false, morekeywords={switch, case, otherwise},
		inputencoding = utf8,  % Input encoding
		extendedchars = true,  % Extended ASCII
		%escapeinside={(*}{*)} % Poder poner scape symbols y también math poniendo (*$...$*) 
	}
}

% -------------------------------------------------------------------------------------------------
% EjemploCodigo_ImgDcha: cuadro para ejemplos con código con una imagen en la mitad derecha
% Para utilizarlo:
% \begin{EjemploCodigoImgDcha}[Anchura imagen]{Titulo del cuadro}{archivo de imagen}{label}
%  ... codigo fuente a mostrar
% \end{EjemploCodigoImgDcha}
% [Anchura imagen]: por ejemplo, 0.7\linewidth. Por defecto vale \linewidth
% {Titulo del cuadro}: el título que aparecerá sobre el cuadro
% {archivo de imagen}: ruta completa del fichero de imagen que aparecerá en la parte derecha del cuadro
%                      un ejemplo podría ser: img/mi_imagen.png
% {etiqueta}: la etiqueta que figurará como label para poder referirse al cuadro con \ref{etiqueta}. 
%             No permite guiones bajos
% -------------------------------------------------------------------------------------------------
\newtcblisting[auto counter, use counter from=EjemploCodigo]{EjemploCodigoImgDcha}[4][\linewith]{
	bicolor, sharp corners, arc=15pt, colframe=black, colback=gray!17, colbacklower=white,
	bottomrule=0.8pt, toprule=0.8pt, leftrule=0.8pt, rightrule=0.8pt,
	left=2pt, top=2pt, bottom=2pt, right=2pt,
	fonttitle=\small\sffamily\bfseries, title={Ejemplo \thetcbcounter},
	attach boxed title to top left,
	label={#4},
	boxed title style={
		enhanced, sharp corners, colframe=black, colback=black,	arc=5pt, bottomrule=0pt, rightrule=0pt,
	},
	interior style={},
	frame style={
		left color=black, right color=black, fill=none
	},
	overlay unbroken and first={
		\node[anchor=west,font=\textbf\sffamily,text width=10cm]
		at (title.east) {{\strut\color{black} #2}};
	},
	listing and comment, sidebyside, righthand ratio=0.5, 
	listing options= {
		language=Rust, frame=none, basicstyle=\footnotesize\ttfamily,
		aboveskip=0pt,belowskip=0pt, lineskip=-0.1em, showstringspaces=false, 
		morekeywords={switch, case, otherwise},
		inputencoding = utf8,  % Input encoding
		extendedchars = true,  % Extended ASCII
		%escapeinside={(*}{*)} % Poder poner scape symbols y también math poniendo (*$...$*)
	},
	comment = {
		%\centering{\tiny\textbf{Salida gráfica} \par}
		\centering\includegraphics[width=#1]{#3}
	}
}



